programa
/*51) Escreva um algoritmo para imprimir os números de 1 (inclusive) a 10 (inclusive) em ordem 
decrescente. */
{
	funcao inicio()
	{
		para(inteiro x=10; x>=1; x--)
		escreva("\n", x)
	}
}
